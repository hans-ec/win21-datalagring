<Window x:Class="_00_AsyncAwait_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_00_AsyncAwait_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="btnBlockingCode" Content="Blockerande kod (Sync)" Margin="10" Padding="20 10" Click="btnBlockingCode_Click" />
            <Button x:Name="btnNonBlockingCode" Content="Icke blockerande kod (Async)" Margin="10" Padding="20 10" Click="btnNonBlockingCode_Click" />
            <Label x:Name="lbResult" Margin="0 20 0 0" />
        </StackPanel>
    </Grid>
</Window>
